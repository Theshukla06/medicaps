{
  "version": 2,
  "builds": [
    {
      "src": "learning/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "learning/wsgi.py"
    }
  ],
  "functions": {
    "migrate": {
      "runtime": "python3.9",
      "memory": 512,
      "includeFiles": ["learning/migrate.py"]
    }
  }
}
